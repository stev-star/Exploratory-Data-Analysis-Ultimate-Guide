    "# array sum\n",
    "def sum(arr):\n",
    "    sum=0\n",
    "\n",
    "    for i in arr:\n",
    "        sum=sum+i\n",
    "    \n",
    "    return(sum)\n",
    "\n",
    "arr=[]\n",
    "\n",
    "arr=[1,2,3]\n",
    "ans=sum(arr)\n",
    "print(ans)\n"
